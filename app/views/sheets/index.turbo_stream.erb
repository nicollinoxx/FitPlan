<%= turbo_stream.replace :next_page_container, partial: "sheet_card", collection: @page.records, as: :sheet %>

<% unless @page.last? %>
  <%= turbo_stream.append :paginables, partial: "shared/paginables/next_page_container", locals: { src: sheets_path(page: @page.next_param, format: :turbo_stream) } %>
<% end %>
